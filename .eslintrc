{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
/**
 * Strict mode
 */
    // babel inserts "use strict"; for us
    // http://eslint.org/docs/rules/strict
    "strict": [0, "never"],

/**
 * ES6
 */
    "no-var": 0,                     // http://eslint.org/docs/rules/no-var

/**
 * Variables
 */
    "no-shadow": 0,                  // http://eslint.org/docs/rules/no-shadow
    "no-shadow-restricted-names": 0, // http://eslint.org/docs/rules/no-shadow-restricted-names
    "no-unused-vars": [0, {          // http://eslint.org/docs/rules/no-unused-vars
      "vars": "local",
      "args": "after-used"
    }],
    "no-use-before-define": 0,       // http://eslint.org/docs/rules/no-use-before-define

/**
 * Possible errors
 */
    "comma-dangle": [0, "never"],    // http://eslint.org/docs/rules/comma-dangle
    "no-cond-assign": [0, "always"], // http://eslint.org/docs/rules/no-cond-assign
    "no-console": 0,                 // http://eslint.org/docs/rules/no-console
    "no-debugger": 0,                // http://eslint.org/docs/rules/no-debugger
    "no-alert": 0,                   // http://eslint.org/docs/rules/no-alert
    "no-constant-condition": 0,      // http://eslint.org/docs/rules/no-constant-condition
    "no-dupe-keys": 0,               // http://eslint.org/docs/rules/no-dupe-keys
    "no-duplicate-case": 0,          // http://eslint.org/docs/rules/no-duplicate-case
    "no-empty": 0,                   // http://eslint.org/docs/rules/no-empty
    "no-ex-assign": 0,               // http://eslint.org/docs/rules/no-ex-assign
    "no-extra-boolean-cast": 0,      // http://eslint.org/docs/rules/no-extra-boolean-cast
    "no-extra-semi": 0,              // http://eslint.org/docs/rules/no-extra-semi
    "no-func-assign": 0,             // http://eslint.org/docs/rules/no-func-assign
    "no-inner-declarations": 0,      // http://eslint.org/docs/rules/no-inner-declarations
    "no-invalid-regexp": 0,          // http://eslint.org/docs/rules/no-invalid-regexp
    "no-irregular-whitespace": 0,    // http://eslint.org/docs/rules/no-irregular-whitespace
    "no-obj-calls": 0,               // http://eslint.org/docs/rules/no-obj-calls
    "no-reserved-keys": 0,           // http://eslint.org/docs/rules/no-reserved-keys
    "no-sparse-arrays": 0,           // http://eslint.org/docs/rules/no-sparse-arrays
    "no-unreachable": 0,             // http://eslint.org/docs/rules/no-unreachable
    "use-isnan": 0,                  // http://eslint.org/docs/rules/use-isnan
    "block-scoped-var": 0,           // http://eslint.org/docs/rules/block-scoped-var
    "eol-last": 0,
    "no-multi-spaces": 0,

/**
 * Best practices
 */
    "consistent-return": 0,          // http://eslint.org/docs/rules/consistent-return
    "curly": [0, "multi-line"],      // http://eslint.org/docs/rules/curly
    "default-case": 0,               // http://eslint.org/docs/rules/default-case
    "dot-notation": [0, {            // http://eslint.org/docs/rules/dot-notation
      "allowKeywords": true
    }],
    "eqeqeq": 0,                     // http://eslint.org/docs/rules/eqeqeq
    "guard-for-in": 0,               // http://eslint.org/docs/rules/guard-for-in
    "no-caller": 0,                  // http://eslint.org/docs/rules/no-caller
    "no-else-return": 0,             // http://eslint.org/docs/rules/no-else-return
    "no-eq-null": 0,                 // http://eslint.org/docs/rules/no-eq-null
    "no-eval": 0,                    // http://eslint.org/docs/rules/no-eval
    "no-extend-native": 0,           // http://eslint.org/docs/rules/no-extend-native
    "no-extra-bind": 0,              // http://eslint.org/docs/rules/no-extra-bind
    "no-fallthrough": 0,             // http://eslint.org/docs/rules/no-fallthrough
    "no-floating-decimal": 0,        // http://eslint.org/docs/rules/no-floating-decimal
    "no-implied-eval": 0,            // http://eslint.org/docs/rules/no-implied-eval
    "no-lone-blocks": 0,             // http://eslint.org/docs/rules/no-lone-blocks
    "no-loop-func": 0,               // http://eslint.org/docs/rules/no-loop-func
    "no-multi-str": 0,               // http://eslint.org/docs/rules/no-multi-str
    "no-native-reassign": 0,         // http://eslint.org/docs/rules/no-native-reassign
    "no-new": 0,                     // http://eslint.org/docs/rules/no-new
    "no-new-func": 0,                // http://eslint.org/docs/rules/no-new-func
    "no-new-wrappers": 0,            // http://eslint.org/docs/rules/no-new-wrappers
    "no-octal": 0,                   // http://eslint.org/docs/rules/no-octal
    "no-octal-escape": 0,            // http://eslint.org/docs/rules/no-octal-escape
    "no-param-reassign": 0,          // http://eslint.org/docs/rules/no-param-reassign
    "no-proto": 0,                   // http://eslint.org/docs/rules/no-proto
    "no-redeclare": 0,               // http://eslint.org/docs/rules/no-redeclare
    "no-return-assign": 0,           // http://eslint.org/docs/rules/no-return-assign
    "no-script-url": 0,              // http://eslint.org/docs/rules/no-script-url
    "no-self-compare": 0,            // http://eslint.org/docs/rules/no-self-compare
    "no-sequences": 0,               // http://eslint.org/docs/rules/no-sequences
    "no-throw-literal": 0,           // http://eslint.org/docs/rules/no-throw-literal
    "no-with": 0,                    // http://eslint.org/docs/rules/no-with
    "radix": 0,                      // http://eslint.org/docs/rules/radix
    "vars-on-top": 0,                // http://eslint.org/docs/rules/vars-on-top
    "wrap-iife": [0, "any"],         // http://eslint.org/docs/rules/wrap-iife
    "yoda": 0,                       // http://eslint.org/docs/rules/yoda

/**
 * Style
 */
    "indent": [0, 2],                // http://eslint.org/docs/rules/
    "brace-style": [0,               // http://eslint.org/docs/rules/brace-style
      "1tbs", {
      "allowSingleLine": true
    }],
    "quotes": [
      0, "single", "avoid-escape"    // http://eslint.org/docs/rules/quotes
    ],
    "camelcase": [0, {               // http://eslint.org/docs/rules/camelcase
      "properties": "never"
    }],
    "comma-spacing": [0, {           // http://eslint.org/docs/rules/comma-spacing
      "before": false,
      "after": true
    }],
    "comma-style": [0, "last"],      // http://eslint.org/docs/rules/comma-style
    "func-names": 0,                 // http://eslint.org/docs/rules/func-names
    "key-spacing": [0, {             // http://eslint.org/docs/rules/key-spacing
        "beforeColon": false,
        "afterColon": true
    }],
    "new-cap": [0, {                 // http://eslint.org/docs/rules/new-cap
      "newIsCap": true
    }],
    "no-multiple-empty-lines": [0, { // http://eslint.org/docs/rules/no-multiple-empty-lines
      "max": 2
    }],
    "no-nested-ternary": 0,          // http://eslint.org/docs/rules/no-nested-ternary
    "no-new-object": 0,              // http://eslint.org/docs/rules/no-new-object
    "no-spaced-func": 0,             // http://eslint.org/docs/rules/no-spaced-func
    "no-trailing-spaces": 0,         // http://eslint.org/docs/rules/no-trailing-spaces
    "no-wrap-func": 0,               // http://eslint.org/docs/rules/no-wrap-func
    "no-underscore-dangle": 0,       // http://eslint.org/docs/rules/no-underscore-dangle
    "one-var": [0, "never"],         // http://eslint.org/docs/rules/one-var
    "padded-blocks": [0, "never"],   // http://eslint.org/docs/rules/padded-blocks
    "semi": [0, "always"],           // http://eslint.org/docs/rules/semi
    "semi-spacing": [0, {            // http://eslint.org/docs/rules/semi-spacing
      "before": false,
      "after": true
    }],
    "space-after-keywords": 0,       // http://eslint.org/docs/rules/space-after-keywords
    "space-before-blocks": 0,        // http://eslint.org/docs/rules/space-before-blocks
    "space-before-function-paren": [0, "never"], // http://eslint.org/docs/rules/space-before-function-paren
    "space-infix-ops": 0,            // http://eslint.org/docs/rules/space-infix-ops
    "space-return-throw-case": 0,    // http://eslint.org/docs/rules/space-return-throw-case
    "spaced-line-comment": 0,        // http://eslint.org/docs/rules/spaced-line-comment
/**
 * JSX style
 */
    "react/display-name": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-quotes": [0, "double"],
    "react/jsx-no-undef": 0,
    "react/jsx-sort-props": 0,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-uses-react": 0,
    "react/jsx-uses-vars": 0,
    "react/no-did-mount-set-state": [0, "allow-in-func"],
    "react/no-did-update-set-state": 0,
    "react/no-multi-comp": 0,
    "react/no-unknown-property": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/self-closing-comp": 0,
    "react/wrap-multilines": 0,
    "react/sort-comp": [0, {
      "order": [
        "displayName",
        "constructor",
        "mixins",
        "statics",
        "propTypes",
        "getDefaultProps",
        "getInitialState",
        "componentWillMount",
        "componentDidMount",
        "componentWillReceiveProps",
        "shouldComponentUpdate",
        "componentWillUpdate",
        "componentWillUnmount",
        "/^_on.+$/",
        "/^on.+$/",
        "/^get.+$/",
        "/^render.+$/",
        "render"
      ]
    }]
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": false,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "jsx": true
  }
}
