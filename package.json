{
  "name": "marshallz",
  "version": "2.0.0",
  "description": "Welcome! And more importantly, Welcome to Marshallz Blog",
  "scripts": {
    "start": "NODE_ENV=development babel-node src/server",
    "build:dist": "NODE_ENV=production babel-node src/webpack",
    "build:lib": "rm -rf lib && mkdir lib && babel src/server -d lib/server && babel src/webpack -d lib/webpack",
    "build": "NODE_ENV=production npm run build:dist && npm run build:lib",
    "test": "NODE_ENV=production node lib/server",
    "serve": "NODE_ENV=production nohup node lib/server &",
    "start:sandbox": "NODE_ENV=test babel-node src/server",
    "build:sandbox": "NODE_ENV=test babel-node src/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:evturn/marshallz.git"
  },
  "author": "Evan Turner",
  "homepage": "https://github.com/evturn/marshallz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evturn/marshallz/issues"
  },
  "babel": {
    "presets": [
      "es2015-webpack",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "cron": "^1.1.0",
    "express": "^4.14.0",
    "mongoose": "^4.5.4",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-observable": "^0.7.2",
    "rxjs": "^5.0.0-beta.10",
    "twitter": "^1.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "postcss-cssnext": "2.7.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.4.1",
    "rimraf": "^2.5.3",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  }
}
