  "scripts": {
    "start": "NODE_ENV=development babel-node src/server",
    "build:dist": "NODE_ENV=production babel-node src/webpack",
    "build:lib": "rm -rf lib && mkdir lib && babel src/server -d lib/server && babel src/webpack -d lib/webpack",
